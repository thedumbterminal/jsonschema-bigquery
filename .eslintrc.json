{
    "env": {
        "node": true,
        "es2022": true,
        "mocha": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:mocha/recommended",
      "standard"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
      "mocha/no-mocha-arrows": 0,
      "comma-dangle": 0
    },
    "plugins": [
        "mocha"
    ]
}
